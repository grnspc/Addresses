{
	"name": "grnspc/addresses",
	"description": "Laravel Addressable is a polymorphic Laravel package, for addressbook management. You can add addresses to any eloquent model with ease.",
	"keywords": [
		"laravel",
		"addresses",
		"addressable"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Nathan Robinson",
			"email": "grnspc.dev@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4|^8.0",
		"laravel/framework": "^8.70 | ^9.0",
		"spatie/laravel-package-tools": "^1.11",
		"rinvex/countries": "^8.1"
	},
	"require-dev": {
		"mockery/mockery": "^1.5",
		"orchestra/testbench": "^7.0",
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"Grnspc\\Addresses\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Grnspc\\Addresses\\Tests\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"test:dox": "phpunit --testdox"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Grnspc\\Addresses\\AddressesServiceProvider"
			]
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
